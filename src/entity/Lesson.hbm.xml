<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Lesson" table="lesson" schema="attendance_management">
        <id name="id" column="ID"/>
        <property name="subjectId" column="SubjectID"/>
        <property name="weekIndex" column="WeekIndex"/>
        <property name="lessonDate" column="LessonDate"/>
    </class>
</hibernate-mapping>